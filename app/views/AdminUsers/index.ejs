<div class="ibox float-e-margins">
    <div>
        <h5><%= success +'ewtfwe8yri' %></h5>

    </div>
    <div class="ibox-content">
        <div class="row">

            <div class="col-sm-4">
                <div class="input-group">
                    <input type="text" placeholder="Search" name="email" value="" class="input-sm form-control">
                    <span class="input-group-btn">
                        <button id="btn-search-by-title" type="submit" class="btn btn-sm btn-primary"> Go!</button>
                    </span>
                </div>
            </div>


            <div class="col-sm-4 m-b-xs">
                <a href="<%= paginate.href({ sort: 'name' }) %>" class="btn btn-primary"><i class="fa fa-pencil-square-o"></i></a>
            </div>
        </div>
        <div class="table-responsive">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th><a href="<%= paginate.href({ sort: 'id' }) %>">id</a></th>
                        <th>email</th>
                        <th>name</th>
                        <th>status</th>
                        <th>Modified</th>
                        <th>Created</th>
                        <th class="actions">
                            <%= 'Actions' %>
                        </th>
                    </tr>
                </thead>
                <tbody>
                    <%  listUsers.forEach((user)=>{  %>
                    <tr>
                        <td>
                            <%= user._id %>
                        </td>
                        <td>
                            <%= user.email %>
                        </td>
                        <td>
                            <%= user.name %>
                        </td>
                        <td>
                            <%= user.status %>
                        </td>
                        <td>
                            <%= user.created_date %>
                        </td>
                        <td>
                            <%= user.updated_date %>
                        </td>
                        <td class="actions">
                            <a class="btn btn-primary" href="/admin-users/view/<%= user._id %>"><i class="fa fa-eye"></i></a>
                            <a class="btn btn-success" href="/admin-users/edit/<%= user._id %>"><i class="fa fa-edit"></i></a>
                            <a class="btn btn-danger" href="#"><i class="fa fa-trash-o"></i></a>
                        </td>
                    </tr>
                    <% }); %>
                </tbody>
            </table>
            <%- include('../element/pagination'); -%>
        </div>

    </div>
</div>
